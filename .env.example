# Environment variables for cheQR

# Base path for deployment (e.g., /cheqr for deploying at domain.com/cheqr)
# Default: /
BASE_PATH=/

# Hide footer credits (set to "true" to hide)
VITE_HIDE_CREDITS=false

# Default QR code preset to load (e.g., "plain", "lyqht")
VITE_DEFAULT_PRESET=plain

# Default data to encode when the app first loads
VITE_DEFAULT_DATA_TO_ENCODE=

# Custom QR code presets JSON string (e.g., '[{"name":"custom","data":"hi"}]')
VITE_QR_CODE_PRESETS=

# Default frame preset name
VITE_FRAME_PRESET=

# Custom frame presets JSON string
VITE_FRAME_PRESETS=

# Disable loading saved settings from localStorage on startup
VITE_DISABLE_LOCAL_STORAGE=false
